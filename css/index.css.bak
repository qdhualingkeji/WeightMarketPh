body{
	margin:0;
}
.userName_warn_bg_div,.phone_warn_bg_div,.pn_warn_bg_div,.agpu_warn_bg_div{
	width: 100%;
	height: 40px;
	line-height: 40px;
	color:#fff;
	text-align:center;
	top:40%; 
	position: fixed;
	display: none;
}
.warn_div{
	margin:auto;
	background-color: #000;
	border-radius:5px;
}
.userName_warn_div{
	width: 100px;
}
.phone_warn_div{
	width: 150px;
}
.pn_warn_div{
	width: 200px;
}
.agpu_warn_div{
	width: 300px;
}
.xxsqyszc_bg_div{
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, 0.2);
	position: fixed;
	display: none;
}
.xxsqyszc_div{
	width: auto;
	top: 20%;
	right: 10%;
	left: 10%;
	padding: 17px 15px 20px;
	background-color: #fff;
	border-radius: 13px;
	position: fixed;
}
.xxsqyszc_div .title_div{
	color: #333;
	font-size: 18px;
	font-weight: 700;
	text-align: center;
}
.xxsqyszc_div .close_span{
	font-size: 20px;
	font-weight: normal;
	float: right;
	margin-top: -8px;
}
.xxsqyszc_div .content_div{
	line-height: 24px;
	margin-top:10px;
	color: #555;
	font-size: 14px;
	white-space: normal;
	word-break: break-all;
	overflow: hidden;
}
.xxsqyszc_div .yszc_a{
	color: #405b95;
	text-decoration: none;
}
.logo_img,.sfhydyxnt_img,.xtys_img{
	width:100%;
}
.tel_div{
	width:53px;
	height:53px;
